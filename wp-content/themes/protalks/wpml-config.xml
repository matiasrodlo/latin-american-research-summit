<wpml-config>
	<elementor-widgets>
		<widget name="protalks_core_icon">
			<fields>
				<field>link</field>
			</fields>
		</widget>
		<widget name="protalks_core_button">
			<fields>
				<field>text</field>
				<field>link</field>
				<field>input_name</field>
			</fields>
		</widget>
		<widget name="protalks_core_twitter_list">
			<fields>
				<field>number_of_items</field>
			</fields>
		</widget>
		<widget name="protalks_core_testimonials_list">
			<fields>
				<field>static_title</field>
			</fields>
		</widget>
		<widget name="protalks_core_accordion_child">
			<fields>
				<field>title</field>
				<field>layout</field>
			</fields>
		</widget>
		<widget name="protalks_core_background_svg">
			<fields>
				<field>svg</field>
			</fields>
		</widget>
		<widget name="protalks_core_banner">
			<fields>
				<field>link_url</field>
				<field>title</field>
				<field>text_field</field>
				<field>text</field>
				<field>input_name</field>
			</fields>
		</widget>
		<widget name="protalks_core_countdown">
			<fields>
				<field>week_label</field>
				<field>week_label_plural</field>
				<field>day_label</field>
				<field>day_label_plural</field>
				<field>hour_label</field>
				<field>hour_label_plural</field>
				<field>minute_label</field>
				<field>minute_label_plural</field>
				<field>second_label</field>
				<field>second_label_plural</field>
			</fields>
		</widget>
		<widget name="protalks_core_counter">
			<fields>
				<field>start_digit</field>
				<field>end_digit</field>
				<field>step_digit</field>
				<field>step_delay</field>
				<field>digit_label</field>
				<field>title</field>
				<field>text</field>
			</fields>
		</widget>
		<widget name="protalks_core_custom_font">
			<fields>
				<field>title</field>
			</fields>
		</widget>
		<widget name="protalks_core_dropcaps">
			<fields>
				<field>letter</field>
				<field>text</field>
			</fields>
		</widget>
		<widget name="protalks_core_google_map">
			<fields>
				<field>address1</field>
				<field>address2</field>
				<field>address3</field>
				<field>address4</field>
			</fields>
		</widget>
		<widget name="protalks_core_highlight">
			<fields>
				<field>text</field>
				<field>highlight_position</field>
				<field>link</field>
			</fields>
		</widget>
		<widget name="protalks_core_icon_list_item">
			<fields>
				<field>link</field>
				<field>title</field>
				<field>space_between_icon_title</field>
			</fields>
		</widget>
		<widget name="protalks_core_icon_with_text">
			<fields>
				<field>link</field>
				<field>title</field>
				<field>text</field>
				<field>button_text</field>
			</fields>
		</widget>
		<widget name="protalks_core_image_marquee">
			<fields>
				<field>duration</field>
				<field>link</field>
			</fields>
		</widget>
		<widget name="protalks_core_image_with_text">
			<fields>
				<field>link</field>
				<field>title</field>
				<field>text</field>
				<field>appear_delay</field>
			</fields>
		</widget>
		<widget name="protalks_core_info_section">
			<fields>
				<field>title</field>
				<field>info_text</field>
				<field>text</field>
				<field>link</field>
				<field>input_name</field>
				<field>background_text_text</field>
			</fields>
		</widget>
		<widget name="protalks_core_pricing_table">
			<fields>
				<field>title</field>
				<field>price</field>
				<field>price_label</field>
				<field>currency</field>
				<field>content</field>
				<field>text</field>
				<field>link</field>
				<field>input_name</field>
			</fields>
		</widget>
		<widget name="protalks_core_progress_bar">
			<fields>
				<field>custom_shape</field>
				<field>duration</field>
				<field>number</field>
				<field>title</field>
			</fields>
		</widget>
		<widget name="protalks_core_section_title">
			<fields>
				<field>title</field>
				<field>subtitle</field>
				<field>decoration_positions</field>
				<field>link</field>
				<field>text</field>
			</fields>
		</widget>
		<widget name="protalks_core_single_image">
			<fields>
				<field>link</field>
				<field>appear_delay</field>
			</fields>
		</widget>
		<widget name="protalks_core_tabs_child">
			<fields>
				<field>tab_title</field>
				<field>layout</field>
			</fields>
		</widget>
		<widget name="protalks_core_text_marquee">
			<fields>
				<field>text_1</field>
				<field>text_2</field>
				<field>text_3</field>
				<field>separator</field>
				<field>duration</field>
				<field>marquee_link</field>
			</fields>
		</widget>
		<widget name="protalks_core_textual_list">
			<fields-in-item items_of="children">
				<field>title</field>
				<field>link</field>
			</fields-in-item>
		</widget>
		<widget name="protalks_core_textual_projects_showcase">
			<fields-in-item items_of="children">
				<field>item_video_src</field>
				<field>item_text</field>
				<field>item_link</field>
			</fields-in-item>
		</widget>
		<widget name="protalks_core_video_button">
			<fields>
				<field>video_link</field>
				<field>play_button_indentation</field>
			</fields>
		</widget>
		<widget name="protalks_core_social_share">
			<fields>
				<field>title</field>
			</fields>
		</widget>
	</elementor-widgets>
</wpml-config>